void setup() {
  pinMode(SONG1, INPUT);
  pinMode(SONG2, INPUT);
  pinMode(SONG3, INPUT);

	// Allow allocation of all timers
	ESP32PWM::allocateTimer(0);
	ESP32PWM::allocateTimer(1);
	ESP32PWM::allocateTimer(2);
	ESP32PWM::allocateTimer(3);
}

void loop() {
  check();
}

void check(){
  if (digitalRead(SONG1) == HIGH && digitalRead(SONG2) == LOW && digitalRead(SONG3) == LOW){
    if (bandera_inicio != 1){
      bandera_inicio = 1;
      menu();
    }
    
  }

  else if (digitalRead(SONG1) == LOW && digitalRead(SONG2) == HIGH && digitalRead(SONG3) == LOW){
    if (bandera_inicio != 2){
      bandera_inicio = 2;
      fight();
    }
  }

  else if (digitalRead(SONG1) == LOW && digitalRead(SONG2) == LOW && digitalRead(SONG3) == HIGH){
    if (bandera_inicio != 3){
      bandera_inicio = 3;
      dead();
    }
  }

}



